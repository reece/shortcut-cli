    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Python: Debug CLI",
            "type": "python",
            "request": "launch",
            "console": "integratedTerminal",
            "justMyCode": true,
            "module": "shortcut_cli.cli",
            "args": [
                "-C", "config.yaml", "-v", "-w", "myome", "import-from-github", "static-data"
            ]
        }

    ]
}